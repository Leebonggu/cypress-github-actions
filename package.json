{
  "name": "cypress-github-actions",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:chrome": "cypress run --browser chrome --config-file cypress/cypress.config.ts",
    "test:": "concurrently --kill-others \"pnpm dev\" \"pnpm cypress:open\"",
    "test:chrome": "concurrently --kill-others \"pnpm dev\" \"pnpm cypress:chrome\"",
    "test": "start-server-and-test 'pnpm start' http://localhost:3000 'pnpm cypress:run'"
    
  },
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "start-server-and-test": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.3.0",
    "msw": "^0.42.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "msw": {
    "workerDirectory": "public"
  }
}